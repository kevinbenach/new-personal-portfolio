{
  "name": "new-personal-portfolio",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "type-check": "tsc --noEmit",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write src/**/*.{js,ts,astro,css,scss,md}",
    "clean": "rm -rf dist .astro",
    "optimize-icons": "svgo --config svgo.config.js -f src/assets/icons -o src/assets/icons",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@astrojs/preact": "^4.1.1",
    "@fontsource/inter": "^5.2.8",
    "@preact/signals": "^2.3.1",
    "astro": "^5.15.1",
    "preact": "^10.27.2"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.5",
    "@astrojs/tailwind": "^6.0.2",
    "@eslint/js": "^9.38.0",
    "@preact/preset-vite": "^2.10.2",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/preact": "^3.2.4",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.5.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/browser": "^4.0.4",
    "@vitest/ui": "^4.0.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.0.0",
    "eslint-plugin-astro": "^0.29.0",
    "globals": "^16.4.0",
    "happy-dom": "^20.0.8",
    "jsdom": "^27.0.1",
    "prettier": "^3.0.0",
    "prettier-plugin-astro": "^0.12.0",
    "svgo": "^4.0.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.0.0",
    "vitest": "^4.0.4"
  }
}